{
  "name": "scheduler",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/scheduler/src",
  "prefix": "vortex-apps",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:webpack-browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/scheduler",
        "index": "apps/scheduler/src/index.html",
        "main": "apps/scheduler/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/scheduler/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/scheduler/src/favicon.ico",
          "apps/scheduler/src/assets"
        ],
        "styles": ["apps/scheduler/src/styles.scss"],
        "scripts": [],
        "customWebpackConfig": {
          "path": "apps/scheduler/webpack.config.js"
        },
        "commonChunk": false
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all",
          "customWebpackConfig": {
            "path": "apps/scheduler/webpack.prod.config.js"
          }
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nrwl/angular:webpack-dev-server",
      "configurations": {
        "production": {
          "browserTarget": "scheduler:build:production"
        },
        "development": {
          "browserTarget": "scheduler:build:development"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "port": 4200,
        "publicHost": "http://localhost:4200"
      }
    },
    "extract-i18n": {
      "executor": "ngx-build-plus:extract-i18n",
      "options": {
        "browserTarget": "scheduler:build",
        "extraWebpackConfig": "apps/scheduler/webpack.config.js"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/scheduler/**/*.ts",
          "apps/scheduler/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/scheduler/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
